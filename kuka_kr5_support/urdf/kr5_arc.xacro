<?xml version="1.0"?>
<!--Generates a urdf from the macro in kr16_2_macro.xacro -->
<robot name="kuka_kr5_arc" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <link name="world"/>

  <xacro:include filename="$(find kuka_kr5_support)/urdf/kr5_arc_macro.xacro" />
  <xacro:kuka_kr5_arc prefix="" parent="world" has_podest="true">
      <origin xyz="0 0 0" rpy="0 0 0" />
  </xacro:kuka_kr5_arc>
</robot>
